    <style name="TabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
        <item name="android:fontFamily">@font/elderly_friendly_fonts</item>
    </style>
    <style name="TextAppearance.Large.Body" parent="TextAppearance.Material3.BodyLarge">
        <item name="android:textSize">@dimen/text_large</item>
        <item name="android:textColor">@color/primary_text_color</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.Large.Headline" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:textSize">@dimen/text_size_extra_large</item>
        <item name="android:textColor">@color/primary_text_color</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="Theme.ElderCareAssistant" parent="Theme.Material3.DayNight.NoActionBar">
        
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorOnPrimary">@color/white</item>
        
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
        
        <item name="textAppearanceHeadlineLarge">@style/TextAppearance.Large.Headline</item>
        <item name="textAppearanceBodyLarge">@style/TextAppearance.Large.Body</item>
        
        
        <item name="minTouchTargetSize">48dp</item>
        
        
        <item name="android:textSize">@dimen/text_large</item>
        <item name="android:textColor">@color/primary_text_color</item>
        
        
        <item name="materialButtonStyle">@style/Widget.ElderCare.Button</item>
    </style>
    <style name="Widget.ElderCare.Button" parent="Widget.Material3.Button">
        <item name="android:minHeight">@dimen/button_min_height</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/button_corner_radius</item>
    </style>
    <style name="Widget.ElderCare.Button.Emergency" parent="Widget.ElderCare.Button">
        <item name="backgroundTint">@color/emergency_button_color</item>
        <item name="strokeColor">@color/emergency_stroke_color</item>
        <item name="strokeWidth">@dimen/stroke_width_medium</item>
    </style>
    <style name="Widget.ElderCare.Button.Outlined" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:minHeight">@dimen/button_min_height</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/button_corner_radius</item>
        <item name="strokeWidth">@dimen/stroke_width_medium</item>
    </style>
    <style name="Widget.ElderCare.Icon">
        <item name="android:layout_width">@dimen/icon_size</item>
        <item name="android:layout_height">@dimen/icon_size</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
